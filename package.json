{
  "name": "haul-rate-part-two",
  "version": "1.0.0",
  "description": "Traffic Density Forecasting POC for delivery optimization",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "build": "tsc",
    "build:frontend": "npm run build --prefix frontend",
    "build:all": "npm run build && npm run build:frontend",
    "start": "node dist/index.js",
    "lint": "eslint src/"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "concurrently": "^9.2.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
