# Routing Provider API Keys (at least one required)
# TomTom API - https://developer.tomtom.com/
TOMTOM_API_KEY=your_tomtom_api_key_here

# HERE API - https://developer.here.com/
HERE_API_KEY=your_here_api_key_here

# Google Maps Platform - https://console.cloud.google.com/google/maps-apis
# Required APIs: Directions API, Geocoding API, Places API
# Note: Google is the most expensive option (~$259/1K optimization runs vs ~$14-22 for TomTom/HERE)
GOOGLE_API_KEY=your_google_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL_HOURS=24
